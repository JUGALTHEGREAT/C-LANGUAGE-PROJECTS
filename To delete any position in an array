#include<stdio.h>
main()
{
	int a[100];
	int i,n,max,position=0;
	
	printf("\n Enter limit==>");
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		printf("\n Enter no==>");
		scanf("%d",&a[i]);
	}
	printf("\n Value\n==>");
	
	    printf("\n Enter position to be deleted");
		scanf("%d",&position);
		for(i=position;i<n;i++)
		{
			a[i]=a[i+1];
		}
       
       for(i=0;i<n-1;i++)
       {
       	printf("\n%d",a[i]);
	   }
}
