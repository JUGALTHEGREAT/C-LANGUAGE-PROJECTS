#include<stdio.h>
main()
{
	int a[100],b[100],c[100],d[100],e[100];
	int i,n1,n2,x,j;
	
	printf("\n Enter limit for A==>");
	scanf("%d",&n1);
	printf("\n Enter limit for B==>");
	scanf("%d",&n2);
	
	for(i=0;i<n1;i++)
	{
		printf("\n Enter no==>");
		scanf("%d",&a[i]);
	}
	printf("\n Value\n==>");
	
	for(i=0;i<n2;i++)
	{
		printf("\n Enter no==>");
		scanf("%d",&b[i]);
	}
	printf("\n Value\n==>");
	
	for(i=0;i<n1;i++)
	{
	     x=a[i];
	     for(j=0;j<n1;j++)
		{
			if(x==b[j])
			{
				printf("\n Common numbers=%d",x);
			}
		}
	}
}
